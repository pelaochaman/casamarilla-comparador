  "modal-trigger#popup-mobile": {
    "children": [
      "modal-layout#popup-mobile"
    ],
    "props": {
      "trigger": "load"
    }
  },
  "modal-layout#popup-mobile": {
    "children": [
      "modal-header",
      "image#popup-mobile"
    ],
    "props": {
      "blockClass": "popup-wrapper"
    }
  },
  "image#popup-mobile": {
    "props": {
