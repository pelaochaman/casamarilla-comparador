  "disclosure-content#sixth": {
    "props": {
      "blockClass": "answer"
    },
    "children": ["rich-text#answer6"]
  },
  "rich-text#question6": {
    "props": {
      "text": "La compra figura como rechazada pero el monto fue descontado de mi tarjeta. ¿Cómo proceder?"
    }
  },
  "rich-text#answer6": {
    "props": {
      "text": "En estos casos, el propio sistema de WebPay realizará el reembolso dentro de las siguientes 72 horas hábiles."
    }
  },

