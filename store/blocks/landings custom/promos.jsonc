    },
    "children": ["rich-text#promos-title", "flex-layout.row#promo-1", "flex-layout.row#promo-2"]
  },
  "rich-text#promos-title": {
    "props": {
      "text": "# Promociones Vigentes",
      "blockClass": "promos-title"
    }
  },
  "flex-layout.row#promo-1": {
    "props": {
